.qa-notification-counter{
	text-align: justify;
	padding: 0px 5px;
	margin-right: 5px;
	cursor: pointer;
    -moz-border-radius:4px;
    -webkit-border-radius:4px;
    border-radius:4px;
}
.qa-notification-none{
	background-color: #ddd;
}
.qa-notification-new{
	background-color: #396E63;
	color: white;
}
a:visited .qa-notification-new{
	color: white;
}

a:visited .qa-notification-none{
	color: black;
}
